{
  "version": 2,
  "builds": [
    { "src": "wsgi.py", "use": "@vercel/python" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "wsgi.py" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|webp))", "dest": "/$1" },
    { "src": "/", "dest": "/index.html" }
  ]
}
